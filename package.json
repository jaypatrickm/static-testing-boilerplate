{
  "name": "static-testing-boilerplate",
  "version": "1.0.0",
  "description": "javascript setup with eslint, prettier, typescript, and husky",
  "main": "index.js",
  "repository": "git@github.com:jaypatrickm/static-testing-boilerplate.git",
  "author": "<jaypatrickm@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "yarn check-format && yarn lint && yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "prettier": "^2.1.2"
  }
}
